{
	"name": "Twitch Auto-Chest Collecter",
	"version": "1.0",
	"description": "Auto collects the chests from twitch streamer's chat",
	"manifest_version": 2,
	"icons": {
		"16": "assets/icon.png",
		"48": "assets/icon.png",
		"128": "assets/icon.png"
	},
	"content_scripts": [
		{
			"matches": ["*://*.twitch.tv/*"],
			"js": ["content.js"]
		}
	]
}
